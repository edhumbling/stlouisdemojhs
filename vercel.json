{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "stlouisdemojhs.com"
        }
      ],
      "destination": "https://www.stlouisdemojhs.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/imagekit-files.js": {
      "maxDuration": 10
    }
  }
}
